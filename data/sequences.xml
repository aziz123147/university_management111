<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <!--    sequence for university student-->
        <record id="seq_university_student" model="ir.sequence">
            <field name="name">student sequence</field>
            <field name="code">university.student.seq</field>
            <field name="prefix">US</field>
            <field name="padding">5</field>
        </record>

        <!--    sequence for university teacher-->
        <record id="seq_university_teacher" model="ir.sequence">
            <field name="name">teacher sequence</field>
            <field name="code">university.teacher.seq</field>
            <field name="prefix">TCH</field>
            <field name="padding">5</field>
        </record>

        <!--    sequence for university department-->
        <record id="seq_university_department" model="ir.sequence">
            <field name="name">department sequence</field>
            <field name="code">university.department.seq</field>
            <field name="prefix">Dept</field>
            <field name="padding">3</field>
        </record>
        <record id="seq_university_classe" model="ir.sequence">
            <field name="name">classe sequence</field>
            <field name="code">university.classe.seq</field>
            <field name="prefix">Cls</field>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>